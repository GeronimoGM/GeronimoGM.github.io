{
  "version": 2,
  "public": true,
  "name": "portfolio",
  "buildCommand": "npm run build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/portfolio/**"
    }
  },
  "redirects": [{ "source": "/", "destination": "/es", "permanent": true }]
}
